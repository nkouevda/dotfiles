                                        "key_code": "rewind"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "left_option + p -> Clear clipboard",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "p",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "printf '' | pbcopy"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "left_option + d -> Copy date to clipboard",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "d",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "date +'%Y-%m-%d' | tr -d '\n' | pbcopy"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
