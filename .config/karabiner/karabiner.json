                            {
                                "from": {
                                    "key_code": "escape",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_control",
                                            "left_option",
                                            "left_command"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "power",
                                        "modifiers": [
                                            "left_option",
                                            "left_command"
                                        ]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Left Option + Fast Forward -> Rewind",
                        "manipulators": [
                            {
                                "from": {
