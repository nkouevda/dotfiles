                                },
                                "to": [
                                    {
                                        "shell_command": "value=\"$(pbpaste)\"; if grep -Eq '^https?://' <<<\"$value\"; then printf '. %s .' \"$value\" | pbcopy; fi"
                                    },
                                    {
                                        "key_code": "vk_none",
                                        "hold_down_milliseconds": 75
