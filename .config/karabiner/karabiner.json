                                    {
                                        "shell_command": "value=\"$(pbpaste)\"; if grep -Eq '^\\. https?://.* \\.$' <<<\"$value\"; then printf '%s' \"$value\" | sed 's/^\\. //; s/ \\.$//' | pbcopy; fi"
