{
    "global": {
        "check_for_updates_on_startup": true,
        "show_in_menu_bar": false,
        "show_profile_name_in_menu_bar": false
    },
    "profiles": [
        {
            "complex_modifications": {
                "parameters": {
                    "basic.to_delayed_action_delay_milliseconds": 500,
                    "basic.to_if_alone_timeout_milliseconds": 1000,
                    "basic.to_if_held_down_threshold_milliseconds": 500
                },
                "rules": [
                    {
                        "description": "Left Control + Left Option + Left Command + Escape -> Lock",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "escape",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_control",
                                            "left_option",
                                            "left_command"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "power",
                                        "modifiers": [
                                            "left_option",
                                            "left_command"
                                        ]
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Left Option + Fast Forward -> Rewind",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "fastforward",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "rewind"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Left Option + P -> Clear clipboard",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "p",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "printf '' | pbcopy"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Left Option + Y -> Copy date to clipboard",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "y",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "date +'%Y-%m-%d' | tr -d '\n' | pbcopy"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "description": "Left Option + <letter> -> Open application",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "a",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /Applications/Messages.app"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "c",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open '/Applications/Google Chrome.app'"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "f",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /System/Library/CoreServices/Finder.app"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "s",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /Applications/Slack.app"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "v",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /usr/local/Cellar/macvim/*/MacVim.app"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "x",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /Applications/iTerm.app"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "z",
                                    "modifiers": {
                                        "mandatory": [
                                            "left_option"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "open /Applications/iTunes.app"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "comma",
